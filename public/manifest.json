{
  "name": "ゼロタブ",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "ゼロタブ"
  },
  "icons": { 
    "32": "favicon-32x32.png",
    "16": "favicon-16x16.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "downloads",
    "contextMenus"
  ]
}